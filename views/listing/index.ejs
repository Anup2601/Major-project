<% layout("/layout/boilerplate") %>
<body>
    <h1>All listings</h1>
    <form action="/listings/new" method="get">
        <button>Create New Listing</button>
    </form>
    <ul>
        <% for(let listing of allistings) { %>
            <li><a href="/listings/<%=listing._id%>/show"><%= listing.title %></a></li>
        <% } %>
    </ul>
    
</body>
 